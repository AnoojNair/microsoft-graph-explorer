{
  "name": "microsoft-graph-explorer",
  "version": "1.0.4",
  "description": "Microsoft Graph Explorer",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "serve": "static-server -p 3000",
    "postinstall": "bower install",
    "build_prod": "npm run build && browserify -s main src/main.js > dist/bundle.js && npm run minify",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js"
  },
  "author": "",
  "devDependencies": {
    "concurrently": "^3.2.0",
    "static-server": "^2.0.3",
    "@types/chai": "^3.4.35",
    "@types/hellojs": "^0.2.31",
    "@types/jquery": "^2.0.40",
    "@types/mocha": "^2.2.39",
    "browserify": "^14.3.0",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "@angular/animations": "^4.0.1",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "angular-in-memory-web-api": "^0.3.0",
    "bower": "^1.8.0",
    "core-js": "^2.4.1",
    "moment": "^2.18.1",
    "rxjs": "^5.0.1",
    "systemjs": "^0.20.12",
    "typescript": "^2.2.1",
    "zone.js": "^0.8.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoftgraph/microsoft-graph-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/microsoftgraph/microsoft-graph-explorer/issues"
  },
  "homepage": "https://github.com/microsoftgraph/microsoft-graph-explorer#readme"
}
