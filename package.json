{
  "name": "microsoft-graph-explorer",
  "version": "1.0.4",
  "description": "Microsoft Graph Explorer",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "serve": "lite-server -c=bs-config.json",
    "postinstall": "bower install",
    "build:prod": "npm run build && browserify --debug -s main src/main.js > dist/bundle.js && npm run minify",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\""
  },
  "author": "",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/hellojs": "^0.2.31",
    "@types/jasmine": "^2.5.47",
    "@types/jquery": "^2.0.40",
    "browserify": "^14.3.0",
    "concurrently": "^3.2.0",
    "jasmine-core": "^2.6.1",
    "lite-server": "^2.3.0",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.2",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "@angular/animations": "~4.0.0",
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/compiler-cli": "^4.1.2",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/platform-server": "^4.1.2",
    "@angular/router": "~4.0.0",
    "angular-in-memory-web-api": "^0.3.0",
    "bower": "^1.8.0",
    "core-js": "^2.4.1",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "moment": "^2.18.1",
    "rxjs": "^5.0.1",
    "systemjs": "^0.20.12",
    "typescript": "^2.2.2",
    "zone.js": "^0.8.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoftgraph/microsoft-graph-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/microsoftgraph/microsoft-graph-explorer/issues"
  },
  "homepage": "https://github.com/microsoftgraph/microsoft-graph-explorer#readme"
}
